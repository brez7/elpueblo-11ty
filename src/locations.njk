<script>
  function initMap() {
    const mapOptions = {
      zoom: 15,
      disableDefaultUI: true,
      gestureHandling: "cooperative",
      styles: [
        { featureType: "poi", stylers: [{ visibility: "off" }] },
        { featureType: "transit", stylers: [{ visibility: "off" }] },
      ],
    };

    const locations = {
      carlsbad: { lat: 33.08228238222282, lng: -117.23670533864193 },
      cardiff: { lat: 33.02750868684086, lng: -117.2732577963117 },
      carmel: { lat: 32.962350103899475, lng: -117.18963311902785 },
      delmar: { lat: 32.980062000320856, lng: -117.24945471165121 },
    };

    for (const [key, loc] of Object.entries(locations)) {
      const map = new google.maps.Map(document.getElementById(`map-${key}`), {
        ...mapOptions,
        center: loc,
      });
      new google.maps.Marker({
        position: loc,
        map: map,
        title: `El Pueblo - ${key.charAt(0).toUpperCase() + key.slice(1)}`,
      });
    }
  }
</script>

